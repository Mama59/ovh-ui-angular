{
  "name": "@oui-angular/message",
  "version": "1.0.0",
  "main": "dist/component.js",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && webpack --progress --colors --hide-modules --config webpack.config.babel.js",
    "karma:watch": "cross-env NODE_ENV=test babel-node ./node_modules/karma/bin/karma start karma.conf.js --watch",
    "karma": "cross-env NODE_ENV=test babel-node ./node_modules/karma/bin/karma start karma.conf.js --reporters spec,coverage-istanbul --single-run",
    "webdriver:update": "webdriver-manager update",
    "protractor": "babel-node $(yarn bin)/protractor",
    "test": "yarn karma && yarn protractor"
  },
  "dependencies": {
    "angular": "^1.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "cross-env": "^3.1.4",
    "karma": "^1.3.0",
    "protractor": "^4.0.14",
    "rimraf": "^2.5.4",
    "webpack": "^1.14.0"
  },
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.10.0",
    "yarn": ">= 0.17.0"
  }
}
